{
    "name": "nextgen-wom-backend",
    "version": "2.7.0",
    "description": "NextGen WOM - Work Order Management Backend API",
    "main": "server.js",
    "scripts": {
        "start": "npm run migrate && node server.js",
        "migrate": "npx sequelize-cli db:migrate",
        "migrate:status": "npx sequelize-cli db:migrate:status",
        "migrate:undo": "npx sequelize-cli db:migrate:undo",
        "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
        "setup": "node scripts/create-visionwest-users.js",
        "dev": "nodemon server.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "seed": "node utils/seeder.js"
    },
    "engines": {
        "node": "18.x"
    },
    "keywords": [
        "work order",
        "management",
        "api"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@getbrevo/brevo": "^3.0.1",
        "aws-sdk": "^2.1572.0",
        "axios": "^1.10.0",
        "bcrypt": "^6.0.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "html2canvas": "^1.4.1",
        "jsonwebtoken": "^9.0.2",
        "jspdf": "^3.0.1",
        "libphonenumber-js": "^1.12.24",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^7.0.9",
        "pdfkit": "^0.17.1",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.35.1",
        "sharp": "^0.34.3"
